		int arr[][]= {
				{95,86},
				{83,92,96},
				{78,83,93,87,88}				
		};
		int sum=0;
		int cnt=0;
		double avg=0.0;
		
		for ( int i = 0 ; i<arr.length;i++) {
			for(int y= 0 ; y<arr[i].length;y++) {
				sum += arr[i][y];
			}
			cnt += arr[i].length;
			avg = sum / cnt;
		}
		
		
		System.out.println(sum);
		System.out.println(cnt);
		
	}
