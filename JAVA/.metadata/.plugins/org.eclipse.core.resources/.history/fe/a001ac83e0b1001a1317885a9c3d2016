package ex5;

public class P182 {

	public static void main(String[] args) {
		int max = 0;
		int[] arr = {1,5,3,8,2};
		
		for(int i = 0 ; i<arr.length;i++) {
			for ( int y = 0 ; y < arr.length ; y++) {
				int tmp = 0 ;
				if(arr[i]<arr[y]) {
					max = arr[i];
				}
			}
		}
		System.out.println(max);
		
	}

}
