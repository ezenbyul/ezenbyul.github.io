package chap06;

public class StudentEx1 {
	public static void main(String[] args) {
		//객체(인스턴스) 생성
		Student st1 = new Student(); 
		st1.name = "윤재섭";
		st1.design = 80;
		st1.ui = 90;
		st1.sw = 40;
		st1.pg = 85;
		st1.getTot();
		System.out.println("총점 : "+st1.tot);
		System.out.println("평균 : "+st1.getAvg());
		Student st2 = new Student("최민석");
		Student st3 = new Student("박성용", 100);
		Student st4 = new Student("박상민", 90, 80);
		Student st5 = new Student("백유안", 100, 90, 95);
		Student st6 = new Student("김병석", 85, 70, 100, 95);
		st2.getTot();
		st3.getTot();
		st4.getTot();
		st5.getTot();
		st6.getTot();
		System.out.println("총점 : "+st2.tot);
		System.out.println("평균 : "+st2.getAvg());
		System.out.println("총점 : "+st3.tot);
		System.out.println("평균 : "+st3.getAvg());
		System.out.println("총점 : "+st4.tot);
		System.out.println("평균 : "+st4.getAvg());
		System.out.println("총점 : "+st5.tot);
		System.out.println("평균 : "+st5.getAvg());
		System.out.println("총점 : "+st6.tot);
		System.out.println("평균 : "+st6.getAvg());
	}
}